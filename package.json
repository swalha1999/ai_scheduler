{
	"name": "bosalieh.com",
	"version": "0.1.0",
	"private": true,
	"packageManager": "pnpm@10.10.0",
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"preinstall": "pnpm approve-builds esbuild sharp",
		"dev": "next dev",
		"build": "pnpm db:migrate && next build",
		"start": "next start",
		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"db:push_dont_use_use_generate_and_migrate": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio --port 6969",
		"db:create": "docker run --name bosalieh-db -e POSTGRES_PASSWORD=mysecretpassword -e POSTGRES_USER=admin -e POSTGRES_DB=main -p 5432:5432 -d postgres:15",
		"db:remove": "pnpm db:stop && docker rm -f bosalieh-db",
		"db:restore": "docker exec -i bosalieh-db bash -c 'gunzip -c /backup.sql.gz | pg_restore -1 --no-owner --clean --if-exists -U root -d bosalieh'",
		"db:copy": "docker cp ./backup/backup.sql.gz bosalieh-db:/ && pnpm db:restore",
		"db:backup": "docker exec -i bosalieh-db bash -c 'pg_dump -U root -d bosalieh -f /backup.sql'",
		"db:setup": "pnpm db:create && sleep 5 && pnpm db:copy && sleep 5 && pnpm db:push",
		"db:setup_clean": "pnpm db:create && sleep 5 && pnpm db:push",
		"db:start": "docker start bosalieh-db",
		"db:stop": "docker stop bosalieh-db",
		"seed": "tsx src/seed/index.ts",
		"knip": "knip",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
